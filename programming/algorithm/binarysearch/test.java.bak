
public class test
{
	public static void main(String[] args) {
		int[] arr = {1,2,3,4,5,6};
		int target = 10;
		int low = 0, high = arr.length;
		while(low <= high) {
			int mid = low + (high-low)/2;
			if(arr[mid] == target) {
				System.out.println("True");
			}
			if(arr[mid] < target) {
				low = mid + 1;
			} else {
				high = mid - 1;
			}
		}
	}
}